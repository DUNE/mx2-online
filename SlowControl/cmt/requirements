package SlowControl
version v4r2
author Cristian Gingu

use MinervaPolicy      v*
use Readout            v4r8  MinervaOnline
use CAEN2718           v*  MinervaOnline

macro_append SlowControllinkopts " $(SlowControlcsopts) "
macro_append SlowControllinkopts " $(CAEN2718csopts) "
macro_append SlowControllinkopts " $(Readoutcsopts) "
macro_append SlowControllinkopts " /r:Readout.dll "
macro_remove cslinkflags "/t:exe"
macro_append cslinkflags " /t:winexe "

application SlowControl *.cs

