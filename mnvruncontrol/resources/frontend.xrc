        <title>Allow control transfer?</title>
        <font>
            <family>default</family>
            <size>15</size>
            <style>normal</style>
            <underlined>0</underlined>
            <weight>normal</weight>
        </font>
        <object class="wxBoxSizer">
            <orient>wxVERTICAL</orient>
            <object class="sizeritem">
                <flag>wxALIGN_CENTER_HORIZONTAL|wxBOTTOM|wxTOP</flag>
                <border>5</border>
                <object class="wxStaticText" name="ltransfer_header">
                    <label>Another client is requesting\ncontrol of the DAQ.</label>
                    <style>wxALIGN_CENTER_HORIZONTAL</style>
                </object>
            </object>
            <object class="sizeritem">
                <option>1</option>
                <flag>wxEXPAND</flag>
                <object class="wxFlexGridSizer">
                    <cols>2</cols>
                    <growablecols>1</growablecols>
                    <growablerows>0,1,2,3</growablerows>
                    <hgap>5</hgap>
                    <rows>4</rows>
                    <vgap>2</vgap>
                    <object class="sizeritem">
                        <object class="wxStaticText" name="transfer_client_identity_label">
                            <label>Identity:</label>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <flag>wxALIGN_RIGHT</flag>
                        <object class="wxStaticText" name="transfer_client_identity">
                            <label>(identity)</label>
                            <font>
                                <family>default</family>
                                <size>15</size>
                                <style>normal</style>
                                <underlined>0</underlined>
                                <weight>bold</weight>
                            </font>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <object class="wxStaticText" name="transfer_client_ip_label">
                            <label>IP address:</label>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <flag>wxALIGN_RIGHT</flag>
                        <object class="wxStaticText" name="transfer_client_ip">
                            <label>(label)</label>
                            <font>
                                <family>default</family>
                                <size>15</size>
                                <style>normal</style>
                                <underlined>0</underlined>
                                <weight>bold</weight>
                            </font>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <flag>wxALIGN_CENTER_VERTICAL</flag>
                        <object class="wxStaticText" name="transfer_client_location_label">
                            <label>Location:</label>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <flag>wxALIGN_CENTER_VERTICAL|wxALIGN_RIGHT</flag>
                        <object class="wxStaticText" name="transfer_client_location">
                            <label>(location)</label>
                            <style>wxALIGN_RIGHT</style>
                            <font>
                                <family>default</family>
                                <size>15</size>
                                <style>normal</style>
                                <underlined>0</underlined>
                                <weight>bold</weight>
                            </font>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <flag>wxALIGN_CENTER_VERTICAL</flag>
                        <object class="wxStaticText" name="transfer_client_phone_label">
                            <label>Phone #:</label>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <flag>wxALIGN_CENTER_VERTICAL|wxALIGN_RIGHT</flag>
                        <object class="wxStaticText" name="transfer_client_phone">
                            <label>(phone #)</label>
                            <style>wxALIGN_RIGHT</style>
                            <font>
                                <family>default</family>
                                <size>15</size>
                                <style>normal</style>
                                <underlined>0</underlined>
                                <weight>bold</weight>
                            </font>
                        </object>
                    </object>
                </object>
            </object>
            <object class="sizeritem">
                <flag>wxALIGN_CENTER_HORIZONTAL|wxBOTTOM|wxTOP</flag>
                <border>10</border>
                <object class="wxStaticText" name="transfer_query">
                    <fg>#238e23</fg>
                    <label>Do you want to transfer control\nto this client?</label>
                    <style>wxALIGN_CENTER_HORIZONTAL</style>
                </object>
            </object>
            <object class="sizeritem">
                <flag>wxALIGN_CENTER_HORIZONTAL</flag>
                <object class="wxBoxSizer">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem">
                        <flag>wxLEFT|wxRIGHT</flag>
                        <border>15</border>
                        <object class="wxButton" name="wxID_YES">
                        </object>
                    </object>
                    <object class="sizeritem">
                        <flag>wxLEFT|wxRIGHT</flag>
                        <border>15</border>
                        <object class="wxButton" name="wxID_NO">
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>